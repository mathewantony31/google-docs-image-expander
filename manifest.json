{
    "name": "Docs: Image Expander",
    "description": "Click-to-expand images in a Google doc",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [{
      "js": ["jquery-3.6.0.min.js","content.js"],
      "css":["style.css"],
      "matches": ["https://docs.google.com/*"]
    }],
    "icons": {
      "16": "/images/icon.png",
      "32": "/images/icon@2x.png",
      "48": "/images/icon@4x.png",
      "128": "/images/icon@8x.png"
  }
}